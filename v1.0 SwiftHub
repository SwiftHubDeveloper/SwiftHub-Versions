local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local myWindow = Rayfield:CreateWindow({
    Name = "SwiftHub",
    Icon = 0,
    LoadingTitle = "Script is loaded",
    LoadingSubtitle = "Welcome to SwiftHub!",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "Swift Hub"
    }
})

